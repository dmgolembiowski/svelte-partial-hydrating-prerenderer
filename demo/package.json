{
  "name": "svelte-partial-hydrating-prerenderer-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "svelvet --hydratable",
    "build": "NODE_ENV=production svelvet --hydratable",
    "serve": "servor ./public",
    "serve:prerendered": "servor ./prerendered",
    "prerender": "npm run build && rm -rf prerendered && cp -R public prerendered && ./node_modules/svelte-partial-hydrating-prerenderer/bin/index.js"
  },
  "dependencies": {
    "svelte": "^3.20.1"
  },
  "devDependencies": {
    "svelvet": "^0.4.0"
  }
}
